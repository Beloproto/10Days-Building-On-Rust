#![no_std]
use soroban_sdk::{
    contract, contractimpl, contracttype, symbol_short, 
    Env, Symbol, Address, String, Map, Vec, vec
};

#[derive(Clone, Debug, Eq, PartialEq)]
#[contracttype]
pub enum EscrowStatus {
    Created,
    FundsDeposited,
    DeliveryConfirmed,
    Disputed,
    Completed,
    Cancelled,
    ArbitrationResolved,
}

